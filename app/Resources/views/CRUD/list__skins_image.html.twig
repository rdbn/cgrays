{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% if object.skins is defined %}
        {% set name = object.skins.name %}
        {% set path = object.skins.image %}
    {% else %}
        {% set name = object.name %}
        {% set path = object.image %}
    {% endif %}

    <div class="col-lg-6">
        <img src="{{ asset(path) }}" alt="{{ name }}" width="50" />
    </div>
{% endblock %}